  <SQLQuery>
    <![CDATA[SELECT mwl.LINE_ID,
         mwl.TYPE,
         mwl.update_time,
         mwml.creation_date,
         mwml.created_by,
         (SELECT user_name
            FROM fnd_user fu
           WHERE fu.user_id = mwml.created_by)
            Creator_Name,
         mwml.last_update_date,
         mwml.last_updated_by,
         (SELECT user_name
            FROM fnd_user fu
           WHERE fu.user_id = mwml.last_updated_by)
            last_updater_Name,
         mwl.WARRANTY_TAG,
         mwl.OLD_SCHEDULE_MAINTAIN_FLAG,
         mwl.NEW_SCHEDULE_MAINTAIN_FLAG,
         mwl.OLD_MAINTAIN_PERIOD,
         mwl.NEW_MAINTAIN_PERIOD,
         mwl.OLD_NEXT_MAINTAIN_DATE,
         mwl.NEW_NEXT_MAINTAIN_DATE
    FROM MNJ_WARR_SCHEDULE_HISTORY_LOG mwl, MNJ_WARRANTY_MANAGEMENT_LINE mwml
   WHERE     mwl.line_id = mwml.line_id
         AND mwl.new_SCHEDULE_MAINTAIN_FLAG = 'Y'
         AND mwl.TYPE = 'LINE'
         AND mwl.OLD_MAINTAIN_PERIOD IS NOT NULL
         AND mwl.OLD_NEXT_MAINTAIN_DATE IS NOT NULL
         AND mwl.OLD_MAINTAIN_PERIOD <> mwl.NEW_MAINTAIN_PERIOD
         AND mwl.OLD_NEXT_MAINTAIN_DATE <> mwl.NEW_NEXT_MAINTAIN_DATE
ORDER BY mwl.update_time DESC]]>
  </SQLQuery>
