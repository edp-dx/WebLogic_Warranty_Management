  <SQLQuery>
    <![CDATA[SELECT   MnjWarrantyManagementLineEO.LINE_ID,
         MnjWarrantyManagementLineEO.HEADER_ID,
         MnjWarrantyManagementLineEO.ORGANIZATION,
         MnjWarrantyManagementLineEO.ORG_ID,
